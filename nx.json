{
  "npmScope": "patterns-example",
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "booking-booking-desktop": {
      "tags": ["scope:booking", "type:app"]
    },
    "booking-booking-desktop-e2e": {
      "tags": [],
      "implicitDependencies": ["booking-booking-desktop"]
    },
    "booking-booking-mobile": {
      "tags": ["scope:booking", "type:app"]
    },
    "booking-booking-mobile-e2e": {
      "tags": [],
      "implicitDependencies": ["booking-booking-mobile"]
    },
    "check-in-check-in-desktop": {
      "tags": ["scope:check-in", "type:app"]
    },
    "check-in-check-in-desktop-e2e": {
      "tags": [],
      "implicitDependencies": ["check-in-check-in-desktop"]
    },
    "check-in-check-in-mobile": {
      "tags": ["scope:check-in", "type:app"]
    },
    "check-in-check-in-mobile-e2e": {
      "tags": [],
      "implicitDependencies": ["check-in-check-in-mobile"]
    }
  }
}
